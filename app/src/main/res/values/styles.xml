<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="Dialog" parent="android:style/Theme.Holo.DialogWhenLarge">
        <item name="android:background">@drawable/selector_dialog</item>
        <item name="android:windowBackground">@drawable/selector_dialog</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="Dialog2" parent="android:style/Theme.Holo.DialogWhenLarge">
        <item name="android:background">@drawable/selector_dialog</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>
</resources>
